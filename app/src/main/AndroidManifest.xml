<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.priyanshparekh.multiplicationtables">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.ScoreActivity"></activity>
        <activity android:name=".activity.QuizLevelActivity" />
        <activity android:name=".activity.TableRangeActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.QuizActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>